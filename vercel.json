{
  "framework": "astro",
  "rewrites": [
    {
      "source": "/hospital",
      "destination": "https://hospital-overrrr.vercel.app/index.html"
    },
    {
      "source": "/hospital/:path*",
      "destination": "https://hospital-overrrr.vercel.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/img/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).webp",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
